
.title {
  text-align: center;
  font-size: 18pt;
  color:rgb(99, 99, 99);
  margin: 80rpx 0;
}
.telplaceholder {
  color:rgb(170, 170, 170);
}
.mobile {
  display: flex;
  position: relative;
}
.mobile .icon {
  width: 48rpx;
  height: 48rpx;
  background-size: 100% 100%;
  background-image:url(data:image/png;base64,);
  margin:0 auto;
  position: absolute;
  left:106rpx;
  bottom: 15rpx;
}
.mobile input {
  border-bottom:1px solid #ccc; 
  width: 470rpx;
  margin:0 auto;
  padding-left: 90rpx;
  height: 80rpx;
  font-size: 17pt;
}
.mobilecancel {
  position: absolute;
  bottom: 15rpx;
  right:100rpx;
  z-index: 999;
}


.vcode {
  text-align: center;
}
.vcode .inpmobile {
  position: relative;
}
.vcode .inpmobile .icon {
  left:0;
  bottom: 0;
  right:265rpx;
  width:40rpx;
  height:40rpx;
}
.vcode .inputvcodes {
  padding:40rpx 0;
  margin:0 auto;
  display: flex;
  justify-content:space-between;
  width: 400rpx;
}
.vcode .inputvcodes input {
  width: 80rpx;
  height:80rpx;
  border:1px solid #ccc;
}
.vcode button {
  width: 600rpx;
  line-height:80rpx;
}
.vcode .vcodeerror {
  line-height: 60rpx;
  width: 500rpx;
  margin:0 auto;
  font-size: 12pt;
  color:red;
  vertical-align: text-top;
}
.vcode .vcodeerror text {
  margin-left: 10rpx;
}