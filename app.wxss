view,text {
  font-size: 24rpx;
}
.openbule {
  color:#fff;
  border-radius: 40rpx;
  width: 240rpx;
  font-size: 14px;
  position: fixed;
  bottom: 120rpx;
  left:50%;
  margin-left:-120rpx;
  line-height:80rpx;
  background: linear-gradient(to right, #3c566d , #3a435a);
  box-shadow: 4rpx 6rpx 10rpx #ccc;
}

.head image{
	width: 100%;
	height: 300rpx;
}

.usericon {
  width:106rpx;
  height:90rpx;
  border:1px solid #eee;
  position:fixed;
  right:0;
  bottom:80rpx;
  border-radius:50rpx 0 0 50rpx;
  display:flex;
  align-items:center;
  box-shadow:0rpx 8rpx 18rpx #ccc;
}
.usericon image {
  margin-left:8rpx;
  width:80rpx;
  height:80rpx;
  margin-top:6rpx;
}

/* footer */
.footer {
  display: flex;
  justify-content:space-around;
  position: fixed;
  bottom: 0;
  border-top:1px solid #d5d5d5;
  width: 100%;
  padding:3px;
  background: #fff;
  z-index: 999;
}
.footer .home,.footer .usercenter {
  display: flex;
  flex-direction:column;

  align-items: center; 
}
.footer text {
  color:#ccc;
  font-size: 22rpx;
}
.footer .home .icon {
  background-size: 100% 100%;
  width: 26px;
  height: 26px;
  background-image: url(data:image/png;base64,);
}
.footer .home.active .icon {
  background-image: url(data:image/png;base64,)
}
.footer .home.active text,.footer .usercenter.active text {
  color:#F28643;
}
.footer .usercenter .icon {
  background-size: 100% 100%;
  width: 26px;
  height: 26px;
  background-image: url(data:image/png;base64,);
}
.footer .usercenter.active .icon {
  background-image: url(data:image/png;base64,);
}